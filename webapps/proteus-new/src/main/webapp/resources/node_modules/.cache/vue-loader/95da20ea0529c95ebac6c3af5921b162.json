ple\n      >\n        <v-list-tile-content>\n          <v-list-tile-title>{{file.title}}</v-list-tile-title>\n        </v-list-tile-content> \n      </v-list-tile>\n      <v-divider :key=\"index\"/>\n    </template>\n    </v-list>\n    </v-card>\n  </section>\n  \n</template>\n\n<script lang=\"js\">\nimport axios from 'axios';\nimport store from './../store/store'\n  export default  {\n    name: 'filelistcomp',\n    store,\n    props: [],\n    mounted() {\n        this.loadData();\n        setInterval(function () {\n          this.loadData();\n        }.bind(this), 1000);\n      \n    },\n    data() {\n      return {\n        fileslist:[],\n        data:[]\n      }\n    },\n    methods: {\n        loadData(){\n            // this.fileslist = [\n            //         {\n            //             \"title\": \"rat_aggregate_stats_1530405703906.csv\",\n            //             \"description\": \"RatAggregateLog\",\n            //             \"link\": \"http://localhost:8080/opsui/data?productID=922ba582-7cc7-11e8-81ea-373255ec0c7a\",\n            //             \"casSource\": \"RatAggregateLog\",\n            //             \"source\": \"RatAggregateLog\",\n            //             \"pubDate\": \"Sun, 01 Jul 2018 06:12:02 IST\"\n            //         }\n            //     ];\n            if(this.currentRepo!=''){\n                axios.get(\"http://localhost:8080/proteus/service/products?topn=10\")\n                .then(response=>{\n                  this.fileslist.splice(0,this.fileslist.length);\n                  response.data.forEach((v, i) => {\n                      const val = (typeof v === 'object') ? Object.assign({}, v) : v;\n                      this.fileslist.push(val);\n                  });\n                          \n                })\n                .catch(error=>{\n                  this.$log.info(error.toString());\n                })\n        \n            }\n            \n        }\n    },\n    computed: {\n      currentRepo (){\n        return store.state.currentRepo;\n      }\n    }\n}\n</script>\n\n<style scoped >\n  #filelistcard{\n    margin-top:10%\n  }\n</style>\n"]}]}